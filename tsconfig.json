{
    "files": [
        "src/index.ts"
    ],
    "compilerOptions": {
        "baseUrl": "./",
        "target": "ES5",
        "preserveConstEnums": true,
        "noImplicitAny": false,
        "lib": ["ES2015", "ES2017", "DOM"],
        "module": "commonjs",
        "moduleResolution": "node",
        "alwaysStrict": true,
        "strict": true,
        "downlevelIteration": true,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "forceConsistentCasingInFileNames": true,
        "importHelpers": true,
        "resolveJsonModule": true,
        "jsx": "react-jsx",
        "paths": {},
        "esModuleInterop":true
    }
}
